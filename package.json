{
  "name": "ic-validator",
  "version": "1.0.0",
  "description": "Simple Typescript validator for Czech business identification number (IČ)",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare" : "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanzelkatomas/ic-validator.git"
  },
  "keywords": [
    "ičo",
    "ič",
    "czech",
    "business",
    "identification",
    "number"
  ],
  "author": "Tomáš Hanzelka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hanzelkatomas/ic-validator/issues"
  },
  "homepage": "https://github.com/hanzelkatomas/ic-validator#readme",
  "devDependencies": {
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.3"
  }
}
